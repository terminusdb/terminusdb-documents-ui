{
  "name": "@terminusdb/terminusdb-documents-ui",
  "version": "1.0.0",
  "description": "SDK to build UI from terminusdb documents",
  "main": "src/index.js",
  "scripts": {
    "build-es6": "rimraf es6 && cross-env NODE_ENV=es6 babel ./src -d es6 --extensions '.js' && babel ./src/css --out-dir es6/css/less --copy-files",
    "build": "npm run build-es6 && rimraf dist && webpack --production ",
    "start": "webpack-dev-server --progress --port 3032 --host localhost --content-base demo --inline --config demo/webpack.config.js",
    "test": "echo 'TODO: There should be tests created'",
    "lint": "eslint src",
    "autofix": "eslint src --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/terminusdb/terminusdb-documents-ui.git"
  },
  "keywords": [
    "terminusdb-documents",
    "terminusdb-documents-ui",
    "terminusdb-ui",
    "terminusdb--sdk"
  ],
  "author": "Kitty Jose",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/terminusdb/terminusdb-documents-ui/issues"
  },
  "homepage": "https://github.com/terminusdb/terminusdb-documents-ui#readme",
