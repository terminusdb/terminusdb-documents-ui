{
  "name": "@terminusdb/terminusdb-documents-ui",
  "version": "1.0.0",
  "description": "SDK to build UI from terminusdb documents",
  "main": "src/index.js",
  "module": "es6/index",
  "jsnext:main": "es6/index",
  "jest": {
    "testEnvironment": "jsdom"
  },
  "scripts": {
    "build-es6": "rimraf es6 && cross-env NODE_ENV=es6 babel ./src -d es6 --extensions '.js' && babel ./src/css --out-dir es6/css/ --copy-files",
    "build": "npm run build-es6 && rimraf dist && webpack --production ",
    "start01": "webpack serve --mode development  --open --progress --port 3032 --host localhost --config demo/webpack.config.js",
    "cams": "webpack serve --mode development  --open --progress --port 3033 --host localhost --config cams/webpack.config.js",
    "geo": "webpack serve --mode development  --open --progress --port 3035 --host localhost --config geo/webpack.config.js",
    "map": "webpack serve --mode development  --open --progress --port 3035 --host localhost --config map/webpack.config.js",
    "covid19": "webpack serve --mode development  --open --progress --port 3034 --host localhost --config covid19/webpack.config.js",
    "experiment2": "webpack serve --mode development  --open --progress --port 3034 --host localhost --config experiment2/webpack.config.js",
    "start": "webpack-dev-server --progress --port 3032 --host localhost --content-base demo --inline --config demo/webpack.config.js",
    "minimal": "webpack-dev-server --progress --port 3031 --host localhost --content-base minimal --inline --config minimal/webpack.config.js",
    "lint": "eslint src",
    "autofix": "eslint src --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/terminusdb/terminusdb-documents-ui.git"
  },
  "keywords": [
    "terminusdb-documents",
    "terminusdb-documents-ui",
    "terminusdb-ui",
    "terminusdb--sdk"
  ],
  "author": "Kitty Jose",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/terminusdb/terminusdb-documents-ui/issues"
  },
  "homepage": "https://github.com/terminusdb/terminusdb-documents-ui#readme",
  "peerDependencies": {
    "react": "^16.10.3",
    "react-dom": "^16.10.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.12.12",
    "@babel/plugin-proposal-export-default-from": "^7.7.4",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-bind": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
    "@babel/plugin-transform-react-jsx": "^7.12.12",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.6.0",
    "@babel/runtime": "^7.6.3",
    "@types/leaflet": "^1.7.8",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.2.3",
    "babel-plugin-lodash": "^3.3.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-airbnb": "^2.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "cross-env": "^5.2.1",
    "css-loader": "^3.6.0",
    "dotenv-webpack": "^1.7.0",
    "eslint": "^8.4.1",
    "eslint-config-airbnb": "^19.0.2",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.0",
    "eslint-plugin-react": "^7.22.0",
    "jest": "^27.4.7",
    "jsdom": "^19.0.0",
    "json-loader": "^0.5.7",
    "less": "^3.13.1",
    "less-loader": "^7.2.1",
    "mini-css-extract-plugin": "^0.9.0",
    "pre-commit": "^1.1.3",
    "rimraf": "^2.7.1",
    "style-loader": "^1.2.1",
    "webpack": "^5.65.0",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.6.0"
  },
  "dependencies": {
    "@rjsf/core": "^3.2.1",
    "babel-jest": "^27.4.6",
    "bootstrap": "^5.1.3",
    "leaflet": "^1.7.1",
    "react": "^16.14.0",
    "react-bootstrap": "^2.0.2",
    "react-bootstrap-typeahead": "^6.0.0-alpha.6",
    "react-dom": "^16.14.0",
    "react-icons": "^4.3.1",
    "react-json-schema-form": "^1.1.2",
    "react-jsonschema-form": "^1.8.1",
    "react-jsonschema-form-extras": "^0.9.67",
    "react-leaflet": "^3.2.5",
    "react-select": "^5.2.1",
    "react-test-renderer": "^17.0.2",
    "regenerator-runtime": "^0.13.9"
  }
}
